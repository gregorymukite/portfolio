{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "manage.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "myproject.settings",
      "SECRET_KEY": "@@SECRET_KEY@@",
      "DEBUG": "false",
      "ALLOWED_HOSTS": "your-vercel-domain.vercel.app"
    },
    "build": {
      "env": {
        "DJANGO_SETTINGS_MODULE": "myproject.settings",
        "SECRET_KEY": "@@SECRET_KEY@@",
        "DEBUG": "false",
        "ALLOWED_HOSTS": "your-vercel-domain.vercel.app"
      }
    }
  }
  